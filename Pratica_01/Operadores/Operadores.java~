package Pr√°tica_01.Operadores;

public class Operadores {
    public static void main(String[] args) {
        Double a = 30.00;
        Double b = 20.00;

        System.out.println(a + b);
        System.out.println(a - b);
        System.out.println(a * b);
        System.out.println(a / b);
    }
}
